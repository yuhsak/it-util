{
  "name": "it-util",
  "version": "0.1.0",
  "description": "Simply work with iterables",
  "author": {
    "name": "Yuhsak Inoue",
    "email": "yuhsak.inoue@gmail.com",
    "url": "https://github.com/Yuhsak"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuhsak/it-util.git"
  },
  "main": "lib/common/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "src",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "*.d.ts.map",
    "tsconfig.json"
  ],
  "scripts": {
    "clean:dist": "rimraf *.js *.js.map *.d.ts *.d.ts.map",
    "clean:test": "rimraf coverage",
    "clean": "run-p clean:*",
    "pretest": "run-p clean:test",
    "test": "jest",
    "check:tsc": "tsc --noEmit",
    "check:prettier": "prettier --check .",
    "check": "run-p check:*",
    "prebuild": "run-p clean:dist",
    "build:watch": "npm run build -- --watch",
    "build": "tsc -b tsconfig.json",
    "start": "npm run watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@swc/core": "^1.2.170",
    "@swc/helpers": "^0.3.8",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "sideEffects": false
}
